{
  "name": "remote-ts",
  "description": "An easy way to remotely add local typescript files across projects.",
  "keywords": ["remote", "typescript", "ts", "import", "copy", "sync"],
  "license": "MIT",
  "version": "1.0.1",
  "module": "index.ts",
  "bin": "dist/index.js",
  "type": "module",
  "packageManager": "pnpm@9.6.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/ping-maxwell/remote-ts"
  },
  "scripts": {
    "build": "tsc",
    "start": "tsc && node dist/index.js",
    "publish": "pnpm publish --no-git-checks"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/bun": "latest",
    "@types/yargs": "^17.0.33"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "chalk": "4.1.2",
    "inquirer": "^12.3.2",
    "yargs": "^17.7.2"
  }
}
